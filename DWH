CREATE TABLE kdz_12_dwh.flights_id (
	fl_date timestamp NOT NULL,
	op_carrier_fl_num varchar(50) NOT NULL,
	origin varchar(50) NOT NULL,
	dest varchar(50) NOT NULL,
	crs_dep_time varchar(50) NOT NULL,
	dwh_id int8 NOT NULL GENERATED ALWAYS AS IDENTITY,
	system_n varchar(50) NOT NULL,
	loaded_ts timestamptz NOT NULL DEFAULT now(),
	CONSTRAINT flights_id_pkey PRIMARY KEY (dwh_id)
);

CREATE TABLE kdz_12_dwh.weather_id (
	local_time timestamp NOT NULL,
	dwh_id int8 NOT NULL GENERATED ALWAYS AS IDENTITY,
	system_n varchar(50) NOT NULL,
	loaded_ts timestamptz NOT NULL DEFAULT now(),
	CONSTRAINT weather_id_pkey PRIMARY KEY (dwh_id)
);

CREATE SEQUENCE kdz_12_dwh.flights_id_dwh_id_seq
	INCREMENT BY 1
	MINVALUE 1
	MAXVALUE 9223372036854775807
	START 1
	CACHE 1
	NO CYCLE;

CREATE SEQUENCE kdz_12_dwh.weather_id_dwh_id_seq
	INCREMENT BY 1
	MINVALUE 1
	MAXVALUE 9223372036854775807
	START 1
	CACHE 1
	NO CYCLE;
